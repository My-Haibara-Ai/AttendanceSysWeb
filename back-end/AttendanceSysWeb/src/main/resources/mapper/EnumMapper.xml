<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zmy.demo.repository.EnumMapper">
    <select id="getIdByStatus" resultType="int">
        select id
        from vac_status
        where status=#{status}
    </select>
</mapper>